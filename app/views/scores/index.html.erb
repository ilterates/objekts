<div class="top-scores">
  <h2> Top Scores </h2> <br>
  <hr class="scores-hr">
  <% @top_scores.each do |s| %>
    <div class="top-scores-name">
      <%= s.username %>
    </div>
    <br>
    <div class="top-scores-score">
      <%= s.top_score %>
    </div>
    <div class="top-scores-date">
      <%= s.created_at.strftime("%m/%d/%Y") %>
    </div>
      <hr class="scores-hr">
  <% end %>
</div>

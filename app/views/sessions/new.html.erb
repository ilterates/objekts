<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-3">Objekts</h1>
  </div>
</div>
<div class="signup-form">
  <%= form_for @user, url: "/sessions", method: "post" do |f| %>
    <div>
      <%= f.text_field :username, placeholder: "Username", class: "form-control form-control-lg" %>
    </div>
    <br>
    <div>
      <%= f.password_field :password, placeholder: "Password", class: "form-control form-control-lg" %>
    </div>
    <br>
    <%= f.submit "Sign In", class:"btn btn-primary" %> <small class="small">Or you can <%= link_to "Sign Up", new_user_path %> </small>
  <% end %>

  <div class="welcome-sphere" id="welcome-sphere">

  </div>
</div>
